#Funci√≥n que calcula el numero de la secuencia de fibonacci de un numero dado
def int fibonacci(int n)
	int x = 0;
	if (n<2)
		x = n;
	else
		#Llamada recursiva con los valores anteriores de n
		x = fibonacci(n-1) + fibonacci(n-2);
	end
	return x;
end

#Programa que imprime el factorial de un numero
main()
	int a = fibonacci(10);
	print(a);
end

#Programa que calcula la secuencia de fibonacci iterativo
main()
	int n1 = 0;
	int n2 = 1;
	int limite = 9;
	int i = 0;
	#Ciclo que imprime los valores de la secuencia
	for(i, limite - 2,1)
		n2 = n1 + n2;
		n1 = n2 - n1;
		print(n2);
	end
end
