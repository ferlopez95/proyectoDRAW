#Función que calcula el numero de la secuencia de fibonacci de un numero dado
def int fibonacci(int n)
	int x = 0;
	if (n<2)
		x = n;
	else
		#Llamada recursiva con los valores anteriores de n
		x = fibonacci(n-1) + fibonacci(n-2);
	end
	return x;
end

#Programa que imprime un número de la secuencia de fibonacci
main()
	int a = fibonacci(10);
	print(a);
end
